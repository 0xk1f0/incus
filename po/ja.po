"### egress: []\n"
"### ingress:\n"
"### - action: allow\n"
"###   state: enabled\n"
"###   protocol: \"\"\n"
"###   source: \"\"\n"
"###   source_port: \"\"\n"
"###   destination: \"\"\n"
"###   destination_port: \"\"\n"
"###   icmp_type: \"\"\n"
